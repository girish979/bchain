		Block genesisBlock = new Block("Block1","00000000");		
		Block block2 = new Block("Block2",genesisBlock.hash);
		Block block3 = new Block("Block3",block2.hash);
		Block block4 = new Block("Block4",block3.hash);
		
		System.out.println("Block 1 hash : " + genesisBlock.hash);
		System.out.println("Block 1 hash : " + genesisBlock.hash);
		System.out.println("Block 1 hash : " + genesisBlock.hash);
		System.out.println("Block 1 hash : " + genesisBlock.hash);
		
	}

}
